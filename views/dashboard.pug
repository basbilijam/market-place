extends ./application.pug
block content
  h3 Dashboard
      ul
        if user.company
          li #{user.company}
          p Location: #{user.location}
          p Cuisine: #{user.style}
          img(src=user.locationurl, width="600" height="450" frameborder="0" style="border:0" allowfullscreen)
          img(src="../uploads/44027290c100d3177b06ac3a47f94e71", width="300" height="225")
        else
          li #{user.username}
